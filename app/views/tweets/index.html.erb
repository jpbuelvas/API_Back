<center>
<h1> Tweets</h1>

<% @tweets.each  do |tweet|%>
    <p><%= tweet.id %> - <%= tweet.description %> - <%= link_to('Ver Detalles', tweet_path(tweet.id)) %> - 
    <%= link_to('Eliminar', tweet_path(tweet.id), method: 'delete') if current_user.id==tweet.user_id%></p>
<%end%>

<%= link_to('Nuevo Tweet', new_tweet_path)%>
</center>
